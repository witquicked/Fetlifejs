{
    "compilerOptions": {
        "declaration": true,
        "stripInternal": true,
        "module": "es2015",
        "moduleResolution": "node",
        "outDir": "./dist",
        "rootDir": "./src",
        "sourceMap": true,
        "inlineSources": true,
        "target": "es5",
        "lib": [
            "es5",
            "es2015",
            "dom"
        ],
        "skipLibCheck": true,
        "noEmitOnError": true
    }
}